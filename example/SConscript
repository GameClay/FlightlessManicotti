Import('env')
bin_env = env.Clone()

# Build test binary
executable_files = ['main.cpp']

example_libs = ['FlightlessManicotti']

if env['KL_CROSS_TARGET']:
   example_libs += env['KL_DEPS']

executable = bin_env.Program('FlightlessManicotti',
   executable_files,
   CPPPATH = [] + env['KL_INCPATH'],
   CXXFLAGS = env['CCFLAGS'],
   LIBS=example_libs, 
   LIBPATH=[] + env['KL_LIBPATH']
)
Return('executable')